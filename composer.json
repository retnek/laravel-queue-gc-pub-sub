{
  "name": "thecubicle/laravel-queue-gc-pub-sub",
  "description": "Laravel queue driver for Google Cloud PubSub",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas Price",
      "email": "taprice@thecubicle.ca"
    }
  ],
  "require": {
    "illuminate/support": "5.5.*",
    "illuminate/queue": "5.5.*",
    "laravel/lumen-framework": "^5.5",
    "google/cloud-pubsub": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "vlucas/phpdotenv": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "thecubicle\\GCPubSub\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "thecubicle\\GCPubSub\\Test\\": "tests/"
    }
  },
  "suggest": {
    "laravel/lumen-framework": "To test the Lumen bindings"
  }
}
